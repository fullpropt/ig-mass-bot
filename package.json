    "bullmq": "^4.16.0",
    "bottleneck": "^2.19.5",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.9",
    "pino": "^8.16.1",
    "pm2": "^5.3.0",
    "proxy-agent": "^6.3.0",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
